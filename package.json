{
  "name": "@3fv/logger",
  "version": "1.1.11",
  "description": "A simply typed logger for typescript",
  "main": "index.js",
  "types": "index.d.ts",
  "scripts": {
    "build": "if [ ! -e index.d.ts ];then mkdir -p lib && cp package.json lib && tsc -b tsconfig.json ${TSC_OPTS}; fi",
    "//build": "mkdir -p lib && cp package.json lib && tsc -b tsconfig.json",
    "build:watch": "cross-env TSC_OPTS=--watch yarn run build",
    "test": "jest",
    "test:watch": "jest --watch",
    "clean": "if test -e lib;then; rm -Rf lib;fi",
    "postversion": "./scripts/release.sh $npm_package_version",
    "release": "yarn build  && yarn version --patch --non-interactive"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/3fv/logger.git"
  },
  "keywords": [
    "typescript"
  ],
  "files": [
    "**/*.d.ts",
    "**/*.js",
    "**/*.map",
    "package.json"
  ],
  "author": "Jonathan Glanz",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/3fv/logger/issues"
  },
  "homepage": "https://github.com/3fv/logger#readme",
  "dependencies": {
    "@3fv/guard": "^1.0.15",
    "@3fv/logger-proxy": "^1.0.1",
    "@3fv/prelude-ts": "^0.8.13",
    "bluebird": "^3.7.2",
    "chalk": "^3.0.0",
    "handlebars": "^4.5.3",
    "moment": "^2.24.0",
    "mz": "^2.7.0"
  },
  "devDependencies": {
    "@types/bluebird": "^3.5.29",
    "@types/jest": "^24.0.23",
    "@types/lodash": "^4.14.149",
    "@types/mz": "^2.7.0",
    "@types/node": "^12.12.17",
    "@types/shelljs": "^0.8.6",
    "cross-env": "^7.0.0",
    "del": "^5.1.0",
    "glob": "^7.1.6",
    "jest": "24.9.0",
    "lodash": "^4.17.15",
    "npm-run-all": "^4.1.5",
    "semver": "^6.3.0",
    "shelljs": "^0.8.3",
    "source-map-support": "^0.5.16",
    "ts-jest": "24.2.0",
    "typescript": "^3.7.3"
  }
}
